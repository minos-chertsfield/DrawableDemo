<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.minos.drawabledemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.RippleActivity"></activity>
        <activity android:name=".activity.AnimationActivity" />
        <activity android:name=".activity.RotateActivity" />
        <activity android:name=".activity.ClipActivity" />
        <activity android:name=".activity.ScaleActivity" />
        <activity android:name=".activity.InsetActivity" />
        <activity android:name=".activity.TransitionActivity" />
        <activity android:name=".activity.StateActivity" />
        <activity android:name=".activity.LevelActivity" />
        <activity android:name=".activity.LayerActivity" />
        <activity android:name=".activity.ShapeActivity" />
        <activity android:name=".activity.NinePatchActivity" />
        <activity android:name=".activity.BitmapActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>